<% if resource.errors.any? %>
  <div id="error_explanation">
    <!-- <ul> -->
      <% resource.errors.full_messages.each do |message| %>
        
        <div class="alert alert-warning alert-dismissible fade show" role="alert">
          <%= message %>
        </div>
      <% end %>
    <!-- </ul> -->
  </div>
<% end %>
